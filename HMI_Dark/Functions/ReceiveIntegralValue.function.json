        "required": false,
        "bindable": true,
        "restParameter": false,
        "asReference": false,
        "allowControlAttributeBindingOptions": false
      },
      {
        "name": "decimalPlaces",
        "displayName": "Decimal Places",
        "type": "tchmi:general#/definitions/INT",
        "description": "Número de casas decimais no resultado do IntGain (0-10)",
        "defaultValue": 4,
        "required": false,
        "bindable": true,
        "restParameter": false,
        "asReference": false,
